<![%MANUSCRIPT;[

<!-- Element:   damage (teitran2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT damage %paraContent;>

<!-- AttList:   damage
     Extend:    None
     Constrain: Require 'agent' and 'extent' attributes.
     -->
<!ATTLIST damage
      %a.global;
      type CDATA #IMPLIED
      extent CDATA #REQUIRED
      resp IDREF %INHERITED;
      hand IDREF %INHERITED;
      agent CDATA #REQUIRED
      degree CDATA #IMPLIED
      TEIform CDATA 'damage'>
]]>

<!-- ========================================================================================== -->

<!-- Element:   date (teicore2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT date %phrase.seq;>

<!-- AttList:   date
     Extend:    Add 'type' attribute with enumerated vocabulary.
     Constrain: Require 'value' attribute; use %ISO-date; as datatype. Change datatype of
                'certainty' from CDATA to enumerated vocabulary.
     Comments:  In values for 'certainty', "ca" stands for Latin circa (around). In values for
                'type', "fl" stands for flourished (or Latin floruit).
     -->
<!ATTLIST date
      %a.global;
      calendar CDATA #IMPLIED
      value %ISO-date; #REQUIRED
      certainty (after | before | ca | exact | uncertain) #IMPLIED
      type (birth | death | fl) #IMPLIED
      TEIform CDATA 'date'>

<!-- ========================================================================================== -->

<!-- Element:   dateline (teistr2.dtd)
     Extend:    Allow hi. Also allow note references (<ref>, <ptr/>, etc.) and notes (<note>).
     Constrain: None
     Comments:  <dateline> is an unusual element in that it allows PCDATA but not hi or other
                phrase-level elements. In this DTD, hi is needed on dateline to allow inclusion of
                rendering information, since we restrict the vocabulary for the global 'rend'
                attribute.
     -->
<!ELEMENT dateline ( #PCDATA | date | time |
      name | address | hi | %m.loc; | %m.notes; | %m.Incl; )*>

<!-- AttList:   dateline
     Extend:    None
     Constrain: None
     -->
<!ATTLIST dateline
      %a.global;
      TEIform CDATA 'dateline'>

<!-- ========================================================================================== -->

<!-- Element:   dateRange (teicore2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT dateRange %phrase.seq;>

<!-- AttList:   dateRange
     Extend:    Add 'type' attribute with enumerated vocabulary.
     Constrain: Require 'from' and 'to'. Use %ISO-date; as datatype.
     Comments:  See comments for 'date' above.
     -->
<!ATTLIST dateRange
      %a.global;
      calendar CDATA #IMPLIED
      from %ISO-date; #REQUIRED
      to %ISO-date; #REQUIRED
      exact (to|from|both|none) #IMPLIED
      type (fl) #IMPLIED
      TEIform CDATA 'dateRange'>

