<![%MANUSCRIPT;[

<!-- Element:   add (teicore2.dtd) (MANUSCRIPT only)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT add %specialPara;>

<!-- AttList:   add
     Extend:    None
     Constrain: Change 'hand' from %INHERITED; to REQUIRED. Change 'place' from CDATA to enumerated
                vocabulary and from IMPLIED to REQUIRED.
     -->
<!ATTLIST add
      %a.global.no_rend;
      rend (%a.rend.display; | %a.rend.align; | %a.rend.indent; | %a.rend.typo; %a.rend.extended;) #IMPLIED
      place (inline|supralinear|infralinear|left|right|top|bottom|opposite|verso|mixed) #REQUIRED
      resp IDREF %INHERITED;
      cert CDATA #IMPLIED
      hand IDREF #REQUIRED
      TEIform CDATA 'add'>
]]>

<!-- ========================================================================================== -->

<!-- Element:   availability (teihdr2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT availability ((p)+)>

<!-- AttList:   availability
     Extend:    None
     Constrain: Change controlled vocabulary for 'status' from (free | unknown | restricted) to
                UVa-specific terms, and change from default value "unknown" to "public".
     -->
<!ATTLIST availability
      %a.global;
      status (public | uva | viva | restricted) "public"
      TEIform CDATA 'availability'>

<!-- ========================================================================================== -->

<!-- Element:   back (teiback2.dtd)
     Extend:    None
     Constrain: Restrict content model to allow only top-level divisions and gaps.
     -->
<!ELEMENT back ( (div1 | %m.editIncl;)+ )>

<!-- AttList:   back
     Extend:    None
     Constrain: None
     -->
<!ATTLIST back
      %a.global;
      %a.declaring;
      TEIform CDATA 'back'>

<!-- ========================================================================================== -->

