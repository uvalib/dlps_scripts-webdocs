<!-- Element:   cit (teicore2.dtd)
     Extend:    None
     Constrain: Restrict content model to enforce intended TEI meaning of cit as a container for a
                single quotation and a single bibliographic reference, in either order.
     -->
<!ELEMENT cit (
    ( (q | quote), (%m.Incl;)*, (%m.bibl; | %m.loc;), (%m.Incl;)* ) |
    ( (%m.bibl; | %m.loc;), (%m.Incl;)*, (q | quote), (%m.Incl;)* )
)>

<!-- AttList:   cit
     Extend:    None
     Constrain: None
     -->
<!ATTLIST cit
      %a.global;
      TEIform CDATA 'cit'>

<!-- ========================================================================================== -->

<![%VENDOR;[
<!ENTITY % closer.ps 'ps |'>
]]>

<!ENTITY % closer.ps ''>

<!-- Element:   closer (teistr2.dtd)
     Extend:    Allow <figure> and <note>. Allow ps (VENDOR only).
     Constrain: None
     Comments:  We have excluded <figure> from m.Incl, but in the case of <closer> there is a
                genuine need for <figure>, so we add it back here.
                Re. adding <note>: See comment for <signed> below.
                <ps> is a convenience element for postscripts within letters (VENDOR only).
     -->
<!ELEMENT closer (#PCDATA | signed | dateline | salute | %m.phrase; | %m.Incl;
      | %closer.ps; figure | note)*>

<!-- AttList:   closer
     Extend:    None
     Constrain: None
     -->
<!ATTLIST closer
      %a.global;
      TEIform CDATA 'closer'>

