<![%POSTKB;[

<!-- Element:   corr (teicore2.dtd) (POSTKB only)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT corr %specialPara;>

<!-- AttList:   corr
     Extend:    None
     Constrain: Change 'resp' datatype from CDATA to IDREF.
     -->
<!ATTLIST corr
      %a.global;
      sic CDATA #IMPLIED
      resp IDREF %INHERITED;
      cert CDATA #IMPLIED
      TEIform CDATA 'corr'>
]]>

