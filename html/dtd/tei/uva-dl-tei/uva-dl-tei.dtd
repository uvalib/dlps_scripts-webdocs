<!-- Element:   cell (teifig2.dtd)
     Extend:    None
     Constrain: Remove <cb/> and <pb/> from content model.
     -->
<!ELEMENT cell (#PCDATA | %m.phrase; | %m.inter; |
                %x.Incl; %n.anchor; | %m.editIncl; | %m.metadata; | %x.refsys; %n.lb; | %n.milestone;)*>

<!-- AttList:   cell
     Extend:    None
     Constrain: Enumerate vocabulary for 'role'.
     -->
<!ATTLIST cell
      %a.global;
      role (data | label) "data"
      rows CDATA "1"
