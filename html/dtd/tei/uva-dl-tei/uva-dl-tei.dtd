                   (((div4) | divGen), (%m.Incl;)*)*)),
              ((%m.divbot;), (%m.Incl;)*)*)>
<!ATTLIST div3
      %a.global;
      %a.divtype;
      %a.declaring;
      TEIform CDATA 'div3'>

<!ELEMENT div4
 ( (%m.divtop; | %m.Incl;)*, ( (((div5) | divGen), (%m.Incl;)*)+
              | ( (%component;, (%m.Incl;)*)+,
                   (((div5) | divGen), (%m.Incl;)*)*)),
              ((%m.divbot;), (%m.Incl;)*)*)>
<!ATTLIST div4
      %a.global;
      %a.divtype;
      %a.declaring;
      TEIform CDATA 'div4'>

<!ELEMENT div5  ( (%m.divtop; | %m.Incl;)*, ( (((div6) | divGen), (%m.Incl;)*)+
              | ( (%component;, (%m.Incl;)*)+,
                   (((div6) | divGen), (%m.Incl;)*)*)),
              ((%m.divbot;), (%m.Incl;)*)*)>
<!ATTLIST div5
      %a.global;
      %a.divtype;
