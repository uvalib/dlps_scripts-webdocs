<![%NONTRANS;[

<!-- Element:   div1 (teistr2.dtd)
     Extend:    None
     Constrain: Restrict content model to allow only page breaks and figures.
     Comments:  The reason for allowing <figure> (in addition to <pb>) is that <figure> is needed
                in cases where the page images are bitonal, but the bitonal image cannot adequately
                represent an illustration, photo, or other graphic.
     -->
<!ELEMENT div1 ( (pb|figure)+ )>

<!-- AttList:   div1
     Extend:    None
     Constrain: Exclude 'type', 'org', 'sample', and 'part' attributes (by removing a.divn).
     -->
<!ATTLIST div1
      %a.global;
      %a.declaring;
      TEIform CDATA 'div1'>

]]>

<!-- ========================================================================================== -->

<!-- Element:   docImprint (teifron2.dtd)
     Extend:    Allow <figure>
     Constrain: None
     Comments:  We have excluded <figure> from m.Incl, but in the case of <docImprint> there is a
                genuine need for <figure>, so we add it back here.
     -->
<!ELEMENT docImprint (#PCDATA | %m.phrase; | pubPlace | docDate | publisher | %m.Incl; | figure)* >

<!-- AttList:   docImprint
     Extend:    None
     Constrain: None
     -->
<!ATTLIST docImprint
      %a.global;
      TEIform CDATA 'docImprint'>

<!-- ========================================================================================== -->

<!-- Element:   extent (teihdr2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT extent %phrase.seq;>

<!-- AttList:   extent
     Extend:    Add 'type' attribute
     Constrain: None
     Comments:  See revision history for version 1.51, 2007-01-16.
     -->
<!ATTLIST extent
      %a.global;
      type (duration) #IMPLIED
      TEIform CDATA 'extent'>

<!-- ========================================================================================== -->

<!-- Element:   figure (teifig2.dtd)
     Extend:    Allow all m.chunk elements, not just <p>. Allow <byline>.
     Constrain: Restrict content model to disallow <text> element.
     Comments:  There are times when elements from class m.chunk, such as <ab> or <lg>, are needed
                within <figure>. It seems odd that TEI would allow <p> here but not other m.chunk
                elements. Adding <byline> allows marking credits for illustrations, photos, etc.
                (without resorting to <p>). Following the example of div1 - div7 (m.divtop class),
                <byline> is allowed at the top of the figure, like a head, before <p> or other
                m.chunk elements.
     -->
<!ELEMENT figure (
    ( (head | byline | %m.Incl;)* )?,
    ( (%m.chunk;), (%m.Incl;)* )*,
    ( figDesc, (%m.Incl;)* )?
)>

<!-- AttList:   figure
     Extend:    Add 'pid' attribute (POSTKB only; see declaration of a.entity class)
     Constrain: Enumerate vocabulary for 'rend'.
     Comments:  'pid' attribute facilitates delivery by making the UVa Library PID (persistent
                identifier) directly available to delivery stylesheets/programs.
     -->
<!ATTLIST figure
      %a.global.no_rend;
      rend (page | block | inline) #IMPLIED
      %a.entity;
      TEIform CDATA 'figure'>

<!-- ========================================================================================== -->

<!-- Element:   fileDesc (teihdr2.dtd)
     Extend:    Allow more than one <publicationStmt> (changed from one to +). Allow more than one
                <seriesStmt> (changed from ? to *).
     Constrain: None
     Comments:  It is possible to have more than one publisher (260a is repeatable in MARC); better
                to have more than one publicationStmt than to put both publishers in one
                publicationStmt and rely on element order to indicate which pubPlace corresponds
                with which <publisher>. It is also possible for an item to participate in more than
                one series.
     -->
<!ELEMENT fileDesc (titleStmt, editionStmt?, extent?,
           publicationStmt+, seriesStmt*, notesStmt?,
           sourceDesc+ )>

<!-- AttList:   fileDesc
     Extend:    None
     Constrain: None
     -->
<!ATTLIST %n.fileDesc;
      %a.global;
      TEIform CDATA 'fileDesc'>

<!-- ========================================================================================== -->

<!-- Element:   front (teifron2.dtd)
     Extend:    None
     Constrain: Restrict content model to allow only <titlePage>, top-level divisions, and gaps.
                For POSTKB only, also allow page breaks. The purpose of this is to allow page breaks
                at the beginning of <front> for covers/edges/spine. Achieving exactly what we want
                (pb|gap followed by titlePage|div1|gap) is impossible without creating an ambiguous
                content model, so we allow pb anywhere and supplement with an XSLT check. See
                revision history entries for versions 1.22, 1.23 and 1.41.
     -->
<![%VENDOR;[
<!ELEMENT front ( (titlePage | div1 | %m.editIncl;)+ )>
]]>

<![%POSTKB;[
<!ELEMENT front ( (titlePage | div1 | %m.editIncl; | pb)+ )>
]]>

<!-- AttList:   front
     Extend:    None
     Constrain: None
     -->
<!ATTLIST front
      %a.global;
      %a.declaring;
      TEIform CDATA 'front'>

<!-- ========================================================================================== -->

<!-- Element:   fw (teitran2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT fw %phrase.seq;>

<!-- AttList:   fw
     Extend:    None
     Constrain: Require 'type'; enumerate vocabulary for 'type' and 'place'.
     -->
<!ATTLIST fw
      %a.global;
      type (catch | footer | header | pageno | sig) #REQUIRED
      place (bottom-center | bottom-left | bottom-right | left | right | top-center | top-left |
             top-right) #IMPLIED
      TEIform CDATA 'fw'>

<!-- ========================================================================================== -->

<!-- Element:   gap (teicore2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT gap EMPTY>

<!-- AttList:   gap
     Extend:    Add 'other' attribute (used when reason="other"). Add altRend attribute (used when
                reason="editorial" and print source has a specific manner of rendering editorial
                omissions, such as multiple asterisks, multiple em dashes, etc.).
     Constrain: Require 'desc' and 'reason' attributes; enumerate vocabulary for 'reason'.
     -->
<!ATTLIST gap
      %a.global;
      altRend CDATA #IMPLIED
      desc CDATA #REQUIRED
      other CDATA #IMPLIED
      resp IDREF %INHERITED;
      hand IDREF %INHERITED;
      agent CDATA #IMPLIED
      extent CDATA #IMPLIED
      TEIform CDATA 'gap'>

<![%VENDOR;[
<!ATTLIST gap reason (damage | editorial | missing | obscured | other) #REQUIRED>
]]>
<![%POSTKB;[
<!ATTLIST gap reason (damage | editorial | missing | obscured | other | unknown) #REQUIRED>
]]>

<!-- ========================================================================================== -->

<!-- Element:   group (teistr2.dtd)
     Extend:    None
     Constrain: Restrict content model to allow only <text> and <group> elements.
     Comments:  Content model requires two or more elements because it makes no sense to have a
                group of one.
     -->
<!ELEMENT group ( (text | group), (text | group)+ )>

<!-- AttList:   group
     Extend:    None
     Constrain: None
     -->
<!ATTLIST group
      %a.global;
      %a.declaring;
      TEIform CDATA 'group'>

<!-- ========================================================================================== -->

<!-- Element:   head (teicore2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT head %paraContent;>

<!-- AttList:   head
     Extend:    None
     Constrain: Enumerate vocabulary for 'type' attribute
     Comments:  We consider 'type' required on <head> when there is more than one <head>, but not
                when just one; so 'type' is not REQUIRED.
     -->
<!ATTLIST head
      %a.global;
      type (alt | desc | divisional | edition | main | resp | series | sub |
            volume) #IMPLIED
      TEIform CDATA 'head'>

<!-- ========================================================================================== -->

<!-- Element:   hi (teicore2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT hi %paraContent;> 

<!-- AttList:   hi
     Extend:    Add 'other' attribute (used when rend="other")
     Constrain: Require 'rend' attribute; enumerate vocabulary
     Comments:  If no value from the enumerated list applies, use <hi rend='other' other='...'>.
-->
<!ATTLIST hi
      %a.global.no_rend;
      rend (%a.rend.display; | %a.rend.align; | %a.rend.indent; | %a.rend.typo; %a.rend.extended; | other) #REQUIRED
      other CDATA #IMPLIED
      TEIform CDATA 'hi'>

<!-- ========================================================================================== -->

<!-- Element:   idno (teihdr2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT idno (#PCDATA)>

<!-- AttList:   idno
     Extend:    None
     Constrain: Require 'type' attribute
     -->
<!ATTLIST idno
      %a.global;
      type CDATA #REQUIRED
      TEIform CDATA 'idno'>

<!-- TODO: Add level attribute with en. vocabulary
      level (monograph | serial) "monograph"
-->

<!-- ========================================================================================== -->

<!-- Element:   language (teihdr2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT language %phrase.seq;>

<!-- AttList:   language
     Extend:    Change 'usage' attribute type from NMTOKEN to CDATA. (NMTOKEN is too restrictive for
                our purposes. I would use an enumerated vocabulary, but we want to use spaces and
                punctuation marks not allowed in the vocab list; vocab can be controlled by an XSLT
                QA stylesheet.)
     Constrain: 
     -->
<!ATTLIST language
      %a.global;
      wsd ENTITY #IMPLIED
      usage CDATA #IMPLIED
      TEIform CDATA 'language'>

<!-- ========================================================================================== -->

<!-- Element:   lg (teicore2.dtd)
     Extend:    Allow <stage>.
     Constrain: None
     Comments:  Needed for verse drama. See revision history entry for 1.52, 2007-04-18.
     -->
<!ELEMENT lg ((%m.divtop; | %m.Incl;)*,
                    (l | lg | stage), (l | lg | stage | %m.Incl;)*,
                    ((%m.divbot;), (%m.Incl;)*)*)>

<!-- AttList:   lg
     Extend:    None
     Constrain: Exclude 'org', 'sample', and 'part' attributes.
     Comments:  Here the 'type' attribute replaces a.divn, which contains 'type', 'org', 'sample',
                and 'part' (and a.metrical, which resolves to an empty string).
     -->

<!ATTLIST lg
      %a.global;
      type CDATA #IMPLIED
      TEIform CDATA 'lg'>

<!-- ========================================================================================== -->

<![%VENDOR;[
<!-- Element:   note (teicore2.dtd)
     Extend:    None
     Constrain: Eliminate mixed content model by disallowing PCDATA and m.phrase, except for
                allowing <ns> (VENDOR) or <seg> (POSTKB) as first element (for recording the note
                symbol).
     -->
<!ELEMENT note ((ns)?, (%m.inter; | %m.chunk; | %m.Incl;)*)>

<!-- AttList:   note
     Extend:    None
     Constrain: Require 'id' and 'place'. Enumerate vocabulary for 'place'.
                Remove 'target' and 'targetEnd' attributes.
                No default value for 'anchored' (change default from "yes" to "IMPLIED").
                Change datatype of 'resp' from CDATA to IDREF.
     Comments:  Removing 'target' and 'targetEnd'
                encourages use of note for body of note, not for
                referencing symbol (for which use <ref> or <ptr/>
                instead). -->
<!ATTLIST note
      %a.global.id_required;
      type CDATA #IMPLIED
      resp IDREF #IMPLIED
      place (above | below | end | foot | head | inline | left | right) #REQUIRED
      anchored (yes | no) #IMPLIED
      TEIform CDATA 'note'>
]]>

<![%POSTKB;[
<!-- Element:   note (teicore2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT note %specialPara;>

<!-- AttList:   note
     Extend:    None
     Constrain: Enumerate vocabulary for 'place'.
                No default value for 'anchored' (change default from "yes" to IMPLIED).
                Change datatype of 'resp' from CDATA to IDREF.
     Comments:  For post-keyboarding DTD, 'target' and 'targetEnd' are available, to allow linking
                from note to point of attachment; 'id' and 'place' attributes are not required,
                because <note> is used in <teiHeader> where 'id' is not necessary (no ref refers to
                the note) and 'place' is not applicable (there is no placement, no print source for
                the note).
     -->
<!ATTLIST note
      %a.global;
      type CDATA #IMPLIED
      resp IDREF #IMPLIED
      place (above | below | end | foot | head | inline | left | right) #IMPLIED
      anchored (yes | no) #IMPLIED
      target IDREFS #IMPLIED
      targetEnd IDREFS #IMPLIED
      TEIform CDATA 'note'>
]]>

<!-- ========================================================================================== -->

<![%MANUSCRIPT;[

<!-- Element:   opener (teicore2.dtd)
     Extend:    Allow new elements <postmark>, <to>, <from>, and <stamp> (MANUSCRIPT only)
     Constrain: None
     Comments:  These elements are needed for identifying the addressing information found on the
                envelope accompanying a manuscript letter.
     -->
<!ELEMENT opener (#PCDATA | %m.phrase; | argument | byline | dateline | epigraph | salute | signed
     | postmark | to | from | stamp | %m.Incl;)* >

<!-- AttList:   opener
     Extend:    None
     Constrain: None
     -->
<!ATTLIST opener
      %a.global;
      TEIform CDATA 'opener'>

]]>

<!-- ========================================================================================== -->

<![%POSTKB;[

<!-- Element:   orig (teicore2.dtd) (POSTKB only)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT orig %phrase.seq;>

<!-- AttList:   orig
     Extend:    None
     Constrain: Change 'resp' datatype from CDATA to IDREF.
     -->
<!ATTLIST orig
      %a.global;
      reg CDATA #IMPLIED
      resp IDREF #IMPLIED
      TEIform CDATA 'orig'>
]]>

<!-- ========================================================================================== -->

<!-- Element:   pb (teicore2.dtd)
     Extend:    Allow <fw>.
     Constrain: None
     Comments:  <pb> is the logical place to record page-related content for which <fw> is used:
                running headers, printing signatures, and page numbers. Using <fw> for page numbers
                in addition to the 'n' attribute follows our practice of not using attributes as
                transcriptional spaces (thus n becomes only a label for display).
     -->
<!ELEMENT pb ((fw)*)>

<!-- AttList:   pb
     Extend:    Add 'entity', 'altEntity', and 'pid' attributes (POSTKB only).
     Constrain: Remove 'rend' attribute.
     Comments:  - 'entity' attribute: added to allow associating page images with the text.
                - 'altEntity' attribute: to be used in cases where page images are bitonal and a
                  grayscale or color figure image exists for a full-page figure. In such a case,
                  the bitonal page image's base filename is the value of the 'entity' attribute;
                  the color/grayscale figure image's base filename is the value of the 'altEntity'
                  attribute. This allows a delivery system to choose between 'entity' for speed or
                  'altEntity' for appearance, as appropriate to the delivery context.
                - 'pid' attribute: facilitates delivery by making the UVa Library PID (persistent
                  identifier) directly available to delivery stylesheets/programs.
                - 'rend' attribute: has no meaning for milestone elements like pb.
     -->
<![%VENDOR;[
<!ATTLIST pb
      %a.global.no_rend;
      ed CDATA #IMPLIED
      TEIform CDATA 'pb'>
]]>

<![%POSTKB;[
<!ATTLIST pb
      %a.global.no_rend;
      ed CDATA #IMPLIED
      %a.entity;
      altEntity ENTITY #IMPLIED
      TEIform CDATA 'pb'>
]]>

<!-- ========================================================================================== -->

<!-- Element:   q (teicore2.dtd)
     Extend:    None: Restore <text> element to content model.
     Constrain: None
     Comments:  <q> is the one element in this DTD (other than TEI.2 and group) where it is
                reasonable to allow <text>. Because <text> has been excluded from m.inter, it must
                be explicitly restored here.
     -->
<!ELEMENT q (#PCDATA | %m.phrase; | %m.inter; | text | %m.chunk; | %m.Incl;)*>

<!-- AttList:   q
     Extend:    None
     Constrain: None
     -->
<!ATTLIST q
      %a.global;
      type CDATA #IMPLIED
      direct (y | n | unspecified) "unspecified"
      who CDATA #IMPLIED
      TEIform CDATA 'q'>

<!-- ========================================================================================== -->

<!-- Element:   ref (teicore2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT ref %paraContent;>

<!-- AttList:   ref
     Extend:    None
     Constrain: Change 'target' from IMPLIED to REQUIRED.
     Comments:  Requiring a reference to refer to something (requiring 'target') is just common
                sense and follows the example of <ptr/>, where 'target' is already required in TEI.
     -->
<!ATTLIST ref
      %a.global;
      %a.pointer;
      target IDREFS #REQUIRED
      TEIform CDATA 'ref'>

<!-- ========================================================================================== -->

<![%POSTKB;[

<!-- Element:   reg (teicore2.dtd) (POSTKB only)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT reg %phrase.seq;>

<!-- AttList:   reg
     Extend:    None
     Constrain: Change 'resp' datatype from CDATA to IDREF.
     -->
<!ATTLIST reg
      %a.global;
      orig CDATA #IMPLIED
      resp IDREF #IMPLIED
      TEIform CDATA 'reg'>
]]>

<!-- ========================================================================================== -->

<!-- Element:   row (teifig2.dtd)
     Extend:    None
     Constrain: Remove <table> from content model. (Nested tables are still allowed within <cell>.)
                Also remove <cb/> and <pb/> from content model.
     -->
<!ELEMENT row (cell, (%x.Incl; %n.anchor; | %m.editIncl; | %m.metadata; | %x.refsys; %n.lb; | %n.milestone;)*)+>

<!-- AttList:   row
     Extend:    None
     Constrain: Remove 'role' attribute.
     Comments:  Use 'role' on cell element instead (e.g. <cell role="label"> is equivalent to <th>
                in XHTML).
     -->
<!ATTLIST row
      %a.global;
      TEIform CDATA 'row'>

<!-- ========================================================================================== -->

<!-- Element:   salute (teistr2.dtd)
     Extend:    Allow <note>
     Constrain: None
     Comments:  See comment for <signed> below.
     -->
<!ELEMENT salute (%phrase; | note)*>

<!-- AttList:   salute
     Extend:    None
     Constrain: None
     -->
<!ATTLIST salute
      %a.global;
      TEIform CDATA 'salute'>

<!-- ========================================================================================== -->

<!-- Element:   seg (teilink2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT seg %paraContent;>

<!-- AttList:   seg
     Extend:    None
     Constrain: Require 'type' attribute and provide enumerated vocabulary. Remove attributes
                'subtype', 'function', and 'part'.
     Comments:  These contraints are accomplished by removing %a.seg; and then defining 'type'.
     -->
<!ATTLIST seg
      %a.global;
      type (anon | call-out | note-symbol | postscript ) #REQUIRED
      TEIform CDATA 'seg'>

<!-- ========================================================================================== -->

<!-- Element:   seriesStmt (teihdr2.dtd)
     Extend:    Allow <biblScope>.
     Constrain: None
     -->
<!ELEMENT seriesStmt ( (title+, (idno | respStmt)*, biblScope*) | p+ )>

<!-- AttList:   seriesStmt
     Extend:    None
     Constrain: None
     -->
<!ATTLIST seriesStmt
      %a.global;
      TEIform CDATA 'seriesStmt'>

<!-- ========================================================================================== -->

<!-- Element:   sic (teicore2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT sic %specialPara;>

<!-- AttList:   sic
     Extend:    None
     Constrain: Change 'resp' datatype from CDATA to IDREF.
     -->
<!ATTLIST sic
      %a.global;
      corr CDATA #IMPLIED
      resp IDREF %INHERITED;
      cert CDATA #IMPLIED
      TEIform CDATA 'sic'>

<!-- ========================================================================================== -->

<!-- Element:   signed (teistr2.dtd)
     Extend:    Allow <note>
     Constrain: None
     Comments:  Notes should be allowed wherever note references (ref or ptr) are allowed. But
                adding <note> to m.loc (which is part of m.phrase) is not possible because doing so
                creates an ambiguous content model (conflicts with <note> in m.inter, I suspect),
                so we have no choice but to add <note> directly to the content models of those
                elements that need it.
     -->
<!ELEMENT signed  (%phrase; | note)*>

<!-- AttList:   signed
     Extend:    None
     Constrain: None
     -->
<!ATTLIST signed
      %a.global;
      TEIform CDATA 'signed'>

<!-- ========================================================================================== -->

<!-- Element:   stage (teicore2.dtd)
     Extend:    None
     Constrain: None
     -->
<!ELEMENT stage %specialPara;>

<!-- AttList:   stage
     Extend:    None
     Constrain: Enumerate vocabulary for 'type' attribute.
     -->
<!ATTLIST stage
      %a.global;
      type (business | delivery | entrance | exit | location |
            modifier | mixed | novelistic | setting) #IMPLIED
      TEIform CDATA 'stage'>

