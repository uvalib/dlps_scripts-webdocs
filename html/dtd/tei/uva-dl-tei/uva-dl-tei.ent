<!-- Class:     a.rend.global (new class)
     Extend:    None
     Constrain: Enumerate vocabulary for global 'rend' attribute.
     -->
<!ENTITY % a.rend.global 'rend (%a.rend.display; | %a.rend.align; | %a.rend.indent;) #IMPLIED'>

<!-- ========================================================================================== -->

<!-- Class:     a.global (teiclas2.ent)
     Extend:    None
     Constrain: Enumerate vocabulary for global 'rend' attribute.
     Comments:  Limiting 'rend' in this way:
                (a) disallows arbitrary rend values ("italics", "italic", "ital" for same feature)
		(b) only allows values for alignment and indentation (not typographic changes)
		(c) requires typographic rendering to be indicated within structural markup
                    (e.g. <p><hi rend="italic">...</hi></p> not <p rend="italic">...</p>).
                For elements that need other 'rend' values, a.global will not be used; instead
                a.global.no_rend will be used and 'rend' will be declared with a different
                enumerated vocabulary.
-->
<!ENTITY % a.global '
      %a.linking;
      id ID #IMPLIED
      n CDATA #IMPLIED
      lang IDREF %INHERITED;
      %a.rend.global;'>

<!-- ========================================================================================== -->

<!-- Class:     a.global.no_rend (new class)
     Extend:    None
     Constrain: (Enumerate vocabulary for 'rend' attribute.)
     Comments:  Identical to a.global except that 'rend' is excluded. Used for elements that need
                to change definition of 'rend' attribute.
     -->
<!ENTITY % a.global.no_rend '
      %a.linking;
      id ID #IMPLIED
      n CDATA #IMPLIED
      lang IDREF %INHERITED;'>

<!-- ========================================================================================== -->

<!-- Class:     a.global.id_required (new class)
     Extend:    None
     Constrain: (Require 'id' attribute.)
     Comments:  Identical to a.global except that 'id' is required.
     -->
<!ENTITY % a.global.id_required '
      %a.linking;
      id ID #REQUIRED
      n CDATA #IMPLIED
      lang IDREF %INHERITED;
      %a.rend.global;'>

<!-- ========================================================================================== -->

<!-- Class:     a.global.n_required (new class)
     Extend:    None
     Constrain: (Require 'n' attribute.)
     Comments:  Identical to a.global except that 'n' is required.
     -->
<!ENTITY % a.global.n_required '
      %a.linking;
      id ID #IMPLIED
      n CDATA #REQUIRED
      lang IDREF %INHERITED;
      %a.rend.global;'>

