<!-- ====================================================================== -->
<!-- characters available in Unicode                                        -->
<!-- ====================================================================== -->

<!ENTITY scripte "&#x212F;"> <!-- SCRIPT SMALL E -->
<!ENTITY lett "&#x026B;">    <!-- LATIN SMALL LETTER L WITH MIDDLE TILDE -->


<!-- ====================================================================== -->
<!-- non-Unicode characters for which an approximation has been made        -->
<!-- ====================================================================== -->

<!ENTITY bl "bl&#x0360;"> <!-- small letters BL strike with tilde -->
	<!-- No Unicode equivalent. Using "bl" plus combining double tilde. -->

<!ENTITY ble "ble&#x0360;"> <!-- small letters BLE strike with tilde -->
	<!-- No Unicode equivalent. Using "ble" plus combining double tilde. -->

<!ENTITY dl "dl&#x0360;"> <!-- small letters DL strike with tilde -->
	<!-- No Unicode equivalent. Using "dl" plus combining double tilde. -->

<!ENTITY GIstrike "G&#x20D2;"> <!-- latin captital letter G strike with I -->
	<!-- No Unicode equivalent. Using "G" plus combining long vertical line overlay. -->

<!ENTITY lord "ll&#x0360;"> <!-- small letters LL strike with tilde -->
	<!-- No Unicode equivalent. Using "ll" plus combining double tilde. -->

<!ENTITY Lord "L&#x0303;"> <!-- capital letter L strike with tilde -->
	<!-- No Unicode equivalent. Using "L" plus combining tilde. -->

<!ENTITY LLord "LL&#x0360;"> <!-- capital letters LL strike with tilde -->
	<!-- No Unicode equivalent. Using "LL" plus combining double tilde. -->

<!ENTITY per "p&#x0331;"> <!-- small letter P strike below -->
	<!-- No Unicode equivalent. Using "p" plus combining macron below. -->

<!ENTITY Per "P&#x0331;"> <!-- capital letter P strike below -->
	<!-- No Unicode equivalent. Using "P" plus combining macron below. -->

<!ENTITY pro "p&#x0333;"> <!-- small letter P with [?] -->
	<!-- No Unicode equivalent. Using "p" plus combining double low line. -->

<!ENTITY pri "p&#x0322;"> <!-- small letter P with [?] -->
	<!-- No Unicode equivalent. Using "p" plus combining retroflex hook below. -->

<!ENTITY que "q&#x0322;"> <!-- small letter Q with [?] -->
	<!-- No Unicode equivalent. Using "q" plus combining retroflex hook below. -->

<!ENTITY ppmacr "p&#x0305;p&#x0305;"> <!-- small letters PP with macron -->
	<!-- No Unicode equivalent. Using "pp" with two combining overlines. -->


